{
    "name": "treehousetim/command",
    "description": "A simple, zero-dependency, shell command class for php.",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Gallagher",
            "email": "treehousetim@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.0",
        "psr/log": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6",
        "monolog/monolog": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "treehousetim\\command\\": "src/"
        }
    },
    "suggest": {
        "monolog/monolog": "PSR compatible logger for logging your command executions",
    }
}
